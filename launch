export PYTHONPATH="$SAGE_ROOT/local/bin:$SAGE_ROOT/local/lib/python2.7/site-packages/setuptools-0.6c8-py2.5.egg:$SAGE_ROOT/local/lib/python2.7/site-packages/SQLAlchemy-0.4.3-py2.5.egg:$SAGE_ROOT/local/bin:$SAGE_ROOT/local/lib/python:$SAGE_ROOT/local/lib/python25.zip:$SAGE_ROOT/local/lib/python2.7/plat-linux2:$SAGE_ROOT/local/lib/python2.7/lib-tk:$SAGE_ROOT/local/lib/python2.7/lib-dynload:$SAGE_ROOT/local/lib/python2.7/site-packages:$SAGE_ROOT/local/lib/python2.7/site-packages/IPython/Extensions:$PYTHONPATH"
python main.py